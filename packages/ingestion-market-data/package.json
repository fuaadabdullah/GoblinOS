{
  "name": "@goblinos/ingestion-market-data",
  "version": "0.1.0",
  "description": "Market data ingestion package for GoblinOS - fetches stock prices from Alpha Vantage, Finnhub, and Polygon",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "market-data",
    "stocks",
    "alpha-vantage",
    "finnhub",
    "polygon",
    "ingestion",
    "goblinos"
  ],
  "author": "GoblinOS",
  "license": "MIT",
  "dependencies": {
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5",
    "@vitest/coverage-v8": "^2.0.5"
  }
}
